---
const githubURL = "https://github.com/"
const youtubeURL = "https://www.youtube.com/"
const linkedinURL = "https://www.linkedin.com/"
const codepenURL = "https://codepen.io/"
---

<div class="social-links my-4">
  <div
    class="grid grid-cols-5 md:grid-cols-8 lg:grid-cols-5 gap-y-3 gap-3 justify-center w-full"
  >
    <a
      href={githubURL}
      class="el flex items-center justify-center aspect-square shadow-xl dark:shadow-thick hover:bg-zinc-800 bg-primary-950 dark:ring-white/10 ring-primary/5 dark:bg-tertiary hover:ring-primary/5 dark:hover:ring-white/20 duration-300 group transition-all h-12 w-12"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        class="icon icon-tabler icon-tabler-brand-github h-6 w-6 group-hover:scale-125 duration-300 text-primary dark:text-white group-hover text-primary:dark:text-white/50"
        viewBox="0 0 24 24"
        stroke-width="0.8"
        stroke="currentColor"
        fill="none"
        stroke-linecap="round"
        stroke-linejoin="round"
        ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
          d="M9 19c-4.3 1.4 -4.3 -2.5 -6 -3m12 5v-3.5c0 -1 .1 -1.4 -.5 -2c2.8 -.3 5.5 -1.4 5.5 -6a4.6 4.6 0 0 0 -1.3 -3.2a4.2 4.2 0 0 0 -.1 -3.2s-1.1 -.3 -3.5 1.3a12.3 12.3 0 0 0 -6.2 0c-2.4 -1.6 -3.5 -1.3 -3.5 -1.3a4.2 4.2 0 0 0 -.1 3.2a4.6 4.6 0 0 0 -1.3 3.2c0 4.6 2.7 5.7 5.5 6c-.6 .6 -.6 1.2 -.5 2v3.5"
        ></path></svg
      >
      <span class="sr-only">Github link</span>
    </a>

    <a
      href={youtubeURL}
      class="el flex items-center justify-center aspect-square shadow-xl dark:shadow-thick hover:bg-zinc-800 bg-primary-950 dark:ring-white/10 ring-primary/5 dark:bg-tertiary hover:ring-primary/5 dark:hover:ring-white/20 duration-300 group transition-all h-12 w-12"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        class="icon icon-tabler icon-tabler-mail h-6 w-6 group-hover:scale-125 duration-300 text-primary dark:text-white group-hover text-primary:dark:text-white/50"
        fill="currentColor"
        viewBox="0 0 24 24"
      >
        <path
          d="M19.615 3.184c-3.604-.246-11.631-.245-15.23 0-3.897.266-4.356 2.62-4.385 8.816.029 6.185.484 8.549 4.385 8.816 3.6.245 11.626.246 15.23 0 3.897-.266 4.356-2.62 4.385-8.816-.029-6.185-.484-8.549-4.385-8.816zm-10.615 12.816v-8l8 3.993-8 4.007z"
        ></path>
      </svg>
      <span class="sr-only">YouTube link</span>
    </a>
    <a
      href="#contact"
      class="el flex items-center justify-center aspect-square shadow-xl dark:shadow-thick hover:bg-zinc-800 bg-primary-950 dark:ring-white/10 ring-primary/5 dark:bg-tertiary hover:ring-primary/5 dark:hover:ring-white/20 duration-300 group transition-all h-12 w-12"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        class="icon icon-tabler icon-tabler-mail h-6 w-6 group-hover:scale-125 duration-300 text-primary dark:text-white group-hover text-primary:dark:text-white/50"
        viewBox="0 0 24 24"
        stroke-width="0.8"
        stroke="currentColor"
        fill="none"
        stroke-linecap="round"
        stroke-linejoin="round"
        ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
          d="M3 7a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v10a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2v-10z"
        ></path><path d="M3 7l9 6l9 -6"></path></svg
      >
      <span class="sr-only">Email link</span>
    </a>

    <a
      href={linkedinURL}
      class="el flex items-center justify-center aspect-square shadow-xl dark:shadow-thick hover:bg-zinc-800 bg-primary-950 dark:ring-white/10 ring-primary/5 dark:bg-tertiary hover:ring-primary/5 dark:hover:ring-white/20 duration-300 group transition-all h-12 w-12"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        class="icon icon-tabler h-6 w-6 group-hover:scale-125 duration-300 text-primary dark:text-white group-hover text-primary:dark:text-white/50 icon-tabler-brand-linkedin"
        viewBox="0 0 24 24"
        stroke-width="0.8"
        stroke="currentColor"
        fill="none"
        stroke-linecap="round"
        stroke-linejoin="round"
        ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
          d="M4 4m0 2a2 2 0 0 1 2 -2h12a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-12a2 2 0 0 1 -2 -2z"
        ></path><path d="M8 11l0 5"></path><path d="M8 8l0 .01"></path><path
          d="M12 16l0 -5"></path><path d="M16 16v-3a2 2 0 0 0 -4 0"></path></svg
      >
      <span class="sr-only">Linkedin link</span>
    </a>
    <a
      href={codepenURL}
      class="el flex items-center justify-center aspect-square shadow-xl dark:shadow-thick hover:bg-zinc-800 bg-primary-950 dark:ring-white/10 ring-primary/5 dark:bg-tertiary hover:ring-primary/5 dark:hover:ring-white/20 duration-300 group transition-all h-12 w-12"
    >
      <!-- codepen icon svg -->
      <svg
        class="icon icon-tabler h-6 w-6 group-hover:scale-125 duration-300 text-primary dark:text-white group-hover text-primary:dark:text-white/50 icon-tabler-brand-codepen"
        xmlns="http://www.w3.org/2000/svg"
        width="1em"
        height="1em"
        viewBox="0 0 24 24"
        stroke-width="2"
        stroke="currentColor"
        fill="none"
        stroke-linecap="round"
        stroke-linejoin="round"
      >
        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
        <path d="M3 15l9 6l9 -6l-9 -6l-9 6"></path>
        <path d="M3 9l9 6l9 -6l-9 -6l-9 6"></path>
        <line x1="3" y1="9" x2="3" y2="15"></line>
        <line x1="21" y1="9" x2="21" y2="15"></line>
        <line x1="12" y1="3" x2="12" y2="9"></line>
        <line x1="12" y1="15" x2="12" y2="21"></line>
      </svg>

      <span class="sr-only">CodePen Link</span>
    </a>
  </div>
</div>
